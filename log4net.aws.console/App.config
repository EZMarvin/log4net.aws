<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <appender name="SimpleDBAppender" type="log4net.Appender.SimpleDBAppender, log4net.Appender.AWS">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender" >
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%ndc] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="SimpleDBAppender" />
    </root>
  </log4net>
  <appSettings>
    <add key="AWSAccessKey" value="AKIAJWGUY6V5Z5K6X2RA"/>
    <add key="AWSSecretKey" value="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"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
